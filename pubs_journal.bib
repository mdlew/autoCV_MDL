  title = {Nanoscale {{Colocalization}} of {{Fluorogenic Probes Reveals}} the {{Role}} of {{Oxygen Vacancies}} in the {{Photocatalytic Activity}} of {{Tungsten Oxide Nanowires}}},
  author = {Shen*, Meikun and Ding*, Tianben and Hartman, Steven T. and Wang, Fudong and Krucylak, Christina and Wang, Zheyu and Tan, Che and Yin, Bo and Mishra, Rohan and Lew$^+$, Matthew D. and Sadtler$^+$, Bryce},
