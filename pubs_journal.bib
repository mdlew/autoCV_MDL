}

@article{shenSingleMoleculeColocalizationRedox2021,
  title = {Single-{{Molecule Colocalization}} of {{Redox Reactions}} on {{Semiconductor Photocatalysts Connects Surface Heterogeneity}} and {{Charge-Carrier Separation}} in {{Bismuth Oxybromide}}},
